{
    "name": "tvdijen/cmdotcom-api-client-bundle",
    "abandoned": true,
    "description": "A Symfony bundle to integrate CM.com's messaging service.",
    "keywords": [
        "cmdotcom",
        "CM.com",
        "sms",
        "surfnet"
    ],
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "tvdijen\\CMDotCom\\": "src"
        }
    },
    "require": {
        "php": ">=7.2 || ^8.0",
        "guzzlehttp/guzzle": "^6.0 || ^7.2",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-kernel": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/phpunit-bridge": "^4.4",
        "vimeo/psalm": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
